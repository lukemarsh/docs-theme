{
  "version": 2,
  "name": "doctocat",
  "alias": "lukesdesignsystem-docs.now.sh",
  "builds": [
    {
      "src": "docs/package.json",
      "use": "@now/static-build",
      "config": {"distDir": "public"}
    }
  ],
  "routes": [
    {"src": "/doctocat(/.*)?", "dest": "/docs$1"},
    {
      "src": "/",
      "status": 301,
      "headers": {"Location": "/doctocat"}
    }
  ]
}
